{
  "chapter": "12. Current Electricity",
  "formulas": [
    {
      "formula_math": "I = \\frac{Q}{t}",
      "formula_english": "Electric current."
    },
    {
      "formula_math": "I = nAqv_d",
      "formula_english": "Current in terms of drift velocity."
    },
    {
      "formula_math": "V = IR",
      "formula_english": "Ohm's law."
    },
    {
      "formula_math": "R = \\rho \\frac{l}{A}",
      "formula_english": "Resistance of conductor."
    },
    {
      "formula_math": "\\rho = \\rho_0 (1 + \\alpha \\Delta T)",
      "formula_english": "Temperature dependence of resistivity."
    },
    {
      "formula_math": "G = \\frac{1}{R}",
      "formula_english": "Conductance."
    },
    {
      "formula_math": "R_s = R_1 + R_2 + ...",
      "formula_english": "Resistors in series."
    },
    {
      "formula_math": "\\frac{1}{R_p} = \\frac{1}{R_1} + \\frac{1}{R_2} + ...",
      "formula_english": "Resistors in parallel."
    },
    {
      "formula_math": "\\varepsilon = I(R + r)",
      "formula_english": "EMF and internal resistance."
    },
    {
      "formula_math": "V = \\varepsilon - Ir",
      "formula_english": "Terminal voltage."
    },
    {
      "formula_math": "P = VI",
      "formula_english": "Electric power."
    },
    {
      "formula_math": "P = I^2 R",
      "formula_english": "Power (current form)."
    },
    {
      "formula_math": "P = \\frac{V^2}{R}",
      "formula_english": "Power (voltage form)."
    },
    {
      "formula_math": "E = Pt",
      "formula_english": "Electrical energy."
    },
    {
      "formula_math": "H = I^2 Rt",
      "formula_english": "Heat produced (Joule's law)."
    },
    {
      "formula_math": "\\sum I = 0",
      "formula_english": "Kirchhoff's current law (junction rule)."
    },
    {
      "formula_math": "\\sum \\varepsilon = \\sum IR",
      "formula_english": "Kirchhoff's voltage law (loop rule)."
    },
    {
      "formula_math": "\\frac{P}{Q} = \\frac{R}{S}",
      "formula_english": "Wheatstone bridge balance condition."
    },
    {
      "formula_math": "R_x = \\frac{R_2}{R_1} R_3",
      "formula_english": "Unknown resistance in Wheatstone bridge."
    },
    {
      "formula_math": "S = \\frac{l}{RA}",
      "formula_english": "Specific conductance."
    }
  ]
}