{
  "chapter": "15. Alternating Current",
  "formulas": [
    {
      "formula_math": "V = V_0 \\sin(\\omega t)",
      "formula_english": "Instantaneous AC voltage."
    },
    {
      "formula_math": "I = I_0 \\sin(\\omega t)",
      "formula_english": "Instantaneous AC current."
    },
    {
      "formula_math": "V_{rms} = \\frac{V_0}{\\sqrt{2}}",
      "formula_english": "RMS voltage."
    },
    {
      "formula_math": "I_{rms} = \\frac{I_0}{\\sqrt{2}}",
      "formula_english": "RMS current."
    },
    {
      "formula_math": "X_L = \\omega L = 2\\pi f L",
      "formula_english": "Inductive reactance."
    },
    {
      "formula_math": "X_C = \\frac{1}{\\omega C} = \\frac{1}{2\\pi f C}",
      "formula_english": "Capacitive reactance."
    },
    {
      "formula_math": "V_L = I_0 X_L",
      "formula_english": "Voltage across inductor."
    },
    {
      "formula_math": "V_C = I_0 X_C",
      "formula_english": "Voltage across capacitor."
    },
    {
      "formula_math": "Z = \\sqrt{R^2 + (X_L - X_C)^2}",
      "formula_english": "Impedance of RLC circuit."
    },
    {
      "formula_math": "I = \\frac{V}{Z}",
      "formula_english": "Current in AC circuit."
    },
    {
      "formula_math": "\\tan \\phi = \\frac{X_L - X_C}{R}",
      "formula_english": "Phase angle."
    },
    {
      "formula_math": "\\cos \\phi = \\frac{R}{Z}",
      "formula_english": "Power factor."
    },
    {
      "formula_math": "P_{avg} = V_{rms} I_{rms} \\cos \\phi",
      "formula_english": "Average power in AC circuit."
    },
    {
      "formula_math": "P = I_{rms}^2 R",
      "formula_english": "Power dissipated in resistance."
    },
    {
      "formula_math": "f_0 = \\frac{1}{2\\pi \\sqrt{LC}}",
      "formula_english": "Resonant frequency."
    },
    {
      "formula_math": "\\omega_0 = \\frac{1}{\\sqrt{LC}}",
      "formula_english": "Angular resonant frequency."
    },
    {
      "formula_math": "Z_{min} = R",
      "formula_english": "Impedance at resonance."
    },
    {
      "formula_math": "I_{max} = \\frac{V_0}{R}",
      "formula_english": "Maximum current at resonance."
    },
    {
      "formula_math": "Q = \\frac{\\omega_0 L}{R} = \\frac{1}{\\omega_0 CR}",
      "formula_english": "Quality factor of RLC circuit."
    }
  ]
}