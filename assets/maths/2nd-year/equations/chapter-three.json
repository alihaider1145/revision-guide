    },
    {
      "eq_eng": "Definite integral",
      "eq_math": "∫_a^b f(x) dx = F(b) - F(a)",
      "related": []
    },
    {
      "eq_eng": "Power rule for integration",
      "eq_math": "∫ x^n dx = x^{n+1}/(n+1) + C",
      "conditions": "n ≠ -1",
      "related": []
    },
    {
      "eq_eng": "Integral of 1/x",
      "eq_math": "∫ (1/x) dx = ln|x| + C",
      "related": []
    },
