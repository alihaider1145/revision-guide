{
  "chapter": "Inverse Trigonometric Functions and Their Graphs",
  "equations": [
    {
      "eq_eng": "Inverse sine function",
      "eq_math": "y = \\sin^{-1} x",
      "conditions": "-1 ≤ x ≤ 1",
      "related": []
    },
    {
      "eq_eng": "Inverse cosine function",
      "eq_math": "y = \\cos^{-1} x",
      "conditions": "-1 ≤ x ≤ 1",
      "related": []
    },
    {
      "eq_eng": "Inverse tangent function",
      "eq_math": "y = \\tan^{-1} x",
      "related": []
    },
    {
      "eq_eng": "Domain–range restriction (inverse sine)",
      "eq_math": "\\sin^{-1}x ∈ [-π/2, π/2]",
      "related": []
    },
    {
      "eq_eng": "Domain–range restriction (inverse cosine)",
      "eq_math": "\\cos^{-1}x ∈ [0, π]",
      "related": []
    },
    {
      "eq_eng": "Composition identity",
      "eq_math": "\\sin(\\sin^{-1}x) = x",
      "conditions": "|x| ≤ 1",
      "related": []
    },
    {
      "eq_eng": "Inverse composition identity",
      "eq_math": "\\sin^{-1}(\\sin x) = x",
      "conditions": "x ∈ [-π/2, π/2]",
      "related": []
    },
    {
      "eq_eng": "Relationship between inverse functions",
      "eq_math": "\\sin^{-1}x + \\cos^{-1}x = \\frac{π}{2}",
      "conditions": "|x| ≤ 1",
      "related": []
    }
  ]
}
