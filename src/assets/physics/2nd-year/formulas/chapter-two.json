{
  "chapter": "12. Current Electricity",
  "formulas": [
    {
      "card_title": "Electric current.",
      "card_math": "I = \\frac{Q}{t}"
    },
    {
      "card_title": "Ohm's law.",
      "card_math": "V = IR"
    },
    {
      "card_title": "Resistance of conductor.",
      "card_math": "R = \\rho \\frac{l}{A}"
    },
    {
      "card_title": "Temperature dependence of resistivity.",
      "card_math": "\\rho = \\rho_0 (1 + \\alpha \\Delta T)"
    },
    {
      "card_title": "Conductance.",
      "card_math": "G = \\frac{1}{R}"
    },
    {
      "card_title": "Resistivity of a material in terms of resistance, length, and cross-sectional area.",
      "card_math": "\\rho = \\frac{RA}{l}"
    },
    {
      "card_title": "Conductivity defined as the reciprocal of resistivity.",
      "card_math": "\\sigma = \\frac{1}{\\rho}"
    },
    {
      "card_title": "Resistors in series.",
      "card_math": "R_s = R_1 + R_2 + ..."
    },
    {
      "card_title": "Resistors in parallel.",
      "card_math": "\\frac{1}{R_p} = \\frac{1}{R_1} + \\frac{1}{R_2} + ..."
    },
    {
      "card_title": "Electromotive force defined as work done per unit charge.",
      "card_math": "\\text{emf} = \\frac{W}{Q}"
    },
    {
      "card_title": "EMF and internal resistance.",
      "card_math": "\\varepsilon = I(R + r)"
    },
    {
      "card_title": "Terminal voltage.",
      "card_math": "V = \\varepsilon - Ir"
    },
    {
      "card_title": "Electric power.",
      "card_math": "P = VI"
    },
    {
      "card_title": "Power (current form).",
      "card_math": "P = I^2 R"
    },
    {
      "card_title": "Power (voltage form).",
      "card_math": "P = \\frac{V^2}{R}"
    },
    {
      "card_title": "Electrical energy.",
      "card_math": "E = Pt"
    },
    {
      "card_title": "Maximum power output of a source when external resistance equals internal resistance.",
      "card_math": "P_{\\text{max}} = \\frac{E^2}{4r}"
    },
    {
      "card_title": "Kirchhoff's current law (junction rule).",
      "card_math": "\\sum I = 0"
    },
    {
      "card_title": "Kirchhoff's voltage law (loop rule).",
      "card_math": "\\sum \\varepsilon = \\sum IR"
    },
    {
      "card_title": "Wheatstone bridge balance condition.",
      "card_math": "\\frac{P}{Q} = \\frac{R}{X}"
    },
    {
      "card_title": "Unknown resistance in Wheatstone bridge.",
      "card_math": "R_x = \\frac{R_2}{R_1} R_3"
    },
    {
      "card_title": "Specific conductance.",
      "card_math": "S = \\frac{l}{RA}"
    }
  ]
}
